{
  "name": "@visionworksco/nodejs-template",
  "version": "0.0.1",
  "private": false,
  "description": "Node.js template",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:visionworksco/nodejs-template.git"
  },
  "author": "VisionWorks",
  "homepage": "/",
  "bugs": {
    "url": "https://github.com/visionworksco/nodejs-template/issues"
  },
  "main": "src/app/index.ts",
  "type": "commonjs",
  "scripts": {
    "build": "npm run clean && npm run compile && tsc -p tsconfig.build.json",
    "clean": "rimraf dist && rimraf coverage",
    "commit": "git add . && git commit -m 'feat: :rocket:'",
    "compile": "tsc --noEmit",
    "compile:diagnostics": "tsc --noEmit --extendedDiagnostics",
    "format": "prettier --write .",
    "lint": "eslint '**/*.{js,ts}' --fix",
    "lint:license": "license-checker --production --json --out licenses.json",
    "lint:node_modules": "./nm-lint.sh",
    "lint:staged": "lint-staged",
    "postinstall": "husky install",
    "setup:app": "node ./src/app/setup.js",
    "setup:app:dev": "cross-env NODE_ENV=development.local npm run setup:app",
    "start": "npm run start:dev:mock",
    "start:dev": "cross-env NODE_ENV=development.local DEBUG=true npm run start:watch",
    "start:dev:mock": "cross-env NODE_ENV=development.local DEBUG=true MOCK_DATA=true npm run start:watch",
    "start:prod": "node dist/app/index.js",
    "start:stage": "cross-env NODE_ENV=development DEBUG=true ts-node .",
    "start:test": "cross-env NODE_ENV=test npm run start:watch",
    "start:watch": "nodemon .",
    "test": "npm run test:unit",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:coverage": "cross-env NODE_ENV=test jest --config jest.config.js --group=unit --coverage",
    "test:dev": "cross-env NODE_ENV=test jest --config jest.config.js --group=unit --watch",
    "test:integration": "cross-env NODE_ENV=test jest --config jest.config.integration.js --group=integration",
    "test:staged": "cross-env NODE_ENV=test jest --config jest.config.integration.js --onlyChanged",
    "test:unit": "cross-env NODE_ENV=test jest --config jest.config.js --group=unit"
  },
  "keywords": [
    "visionworks"
  ],
  "dependencies": {
    "@visionworksco/nodejs-middleware": "^2.1.0",
    "amqplib": "^0.8.0",
    "atob": "^2.1.2",
    "axios": "^0.21.1",
    "chalk": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "envalid": "^6.0.2",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "moment": "^2.29.1",
    "nanoid": "^3.1.28",
    "nconf": "^0.11.3",
    "ora": "^6.0.1",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/prompt-cli": "^16.0.0",
    "@types/amqplib": "^0.8.2",
    "@types/atob": "^2.1.2",
    "@types/express": "^4.17.8",
    "@types/express-serve-static-core": "^4.17.21",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "27.0.3",
    "@types/nconf": "^0.10.1",
    "@types/node": "^14.11.2",
    "@types/pg": "^8.6.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^7.0.4",
    "jest": "^27.1.1",
    "jest-extended": "^1.2.0",
    "jest-mock-extended": "^2.0.4",
    "jest-runner-groups": "^2.1.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^10.4.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.1.2",
    "prettier-plugin-organize-imports": "^1.1.1",
    "prettier-plugin-package": "^1.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.1",
    "ts-node": "^9.0.0",
    "typescript": "4.0.3"
  }
}
