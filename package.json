{
  "name": "@visionworksco/nodejs-template",
  "version": "1.1.1",
  "private": true,
  "description": "Node.js template",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:visionworksco/nodejs-template.git"
  },
  "author": "VisionWorks",
  "homepage": "/",
  "bugs": {
    "url": "https://github.com/visionworksco/nodejs-template/issues"
  },
  "main": "src/app/index.ts",
  "engines": {
    "node": ">=14.18.3"
  },
  "scripts": {
    "build": "npm run clean && npm run compile && tsc -p tsconfig.build.json",
    "clean": "rimraf dist && rimraf coverage",
    "clean:full": "./clean-full.sh",
    "commit": "git add . && git commit -m 'feat: :rocket:'",
    "compile": "tsc --noEmit",
    "compile:diagnostics": "tsc --noEmit --extendedDiagnostics",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "lint": "eslint --fix '**/*.{js,jsx,ts,tsx}'",
    "lint:license": "license-checker --production --json --out licenses.json",
    "lint:nm:audit": "npm audit",
    "lint:nm:deprecated": "./lint-nm-deprecated.sh",
    "lint:nm:unused": "./lint-nm-unused.sh",
    "lint:staged": "lint-staged",
    "postinstall": "husky install",
    "release:first": "standard-version --first-release",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:publish": "git push --follow-tags origin main",
    "setup:app": "ts-node ./src/app/setup/Setup.ts --action=create",
    "setup:app:dev": "cross-env NODE_ENV=development.local npm run setup:app",
    "start": "npm run start:dev:mock",
    "start:dev": "cross-env NODE_ENV=development.local DEBUG=true nodemon .",
    "start:dev:mock": "cross-env NODE_ENV=development.local DEBUG=true MOCK_DATA=true nodemon .",
    "start:prod": "node ./dist/app/index.js",
    "start:stage": "cross-env NODE_ENV=production DEBUG=true ts-node .",
    "start:test": "cross-env NODE_ENV=test nodemon .",
    "test": "npm run test:unit",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:coverage": "jest --group=unit --coverage",
    "test:integration": "jest --group=integration",
    "test:staged": "jest --onlyChanged",
    "test:unit": "jest --group=unit",
    "test:watch": "jest --group=unit --watch"
  },
  "dependencies": {
    "@visionworksco/nodejs-middleware": "^2.2.1",
    "amqplib": "^0.8.0",
    "axios": "^0.21.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^8.0.3",
    "express": "^4.17.1",
    "mongoose": "^6.3.0",
    "multer": "^1.4.4",
    "nanoid": "^3.3.2",
    "nconf": "^0.11.3",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/prompt-cli": "^16.0.0",
    "@types/amqplib": "^0.8.2",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.21",
    "@types/jest": "27.0.3",
    "@types/multer": "^1.4.7",
    "@types/nconf": "^0.10.1",
    "@types/node": "^14.11.2",
    "@types/pg": "^8.6.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "cross-env": "^7.0.2",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^7.0.4",
    "jest": "^27.1.1",
    "jest-extended": "^1.2.0",
    "jest-mock-extended": "^2.0.4",
    "jest-runner-groups": "^2.1.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^10.4.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.1.2",
    "prettier-plugin-organize-imports": "^1.1.1",
    "prettier-plugin-package": "^1.0.0",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.6.2"
  }
}
